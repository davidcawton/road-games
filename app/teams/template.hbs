<div class="nav">
  {{#link-to 'index'}}<p class="nav-title">The Road Game</p>{{/link-to}}
  <img class="nav-logo" src="/team-logos/map.jpg" alt="">
  <ul class="nav-ul">
    <li class="nav-li">
      {{#link-to 'login' }}Login{{/link-to}}
    </li>
    <li class="nav-li">
      {{#link-to 'admin'}}Admin{{/link-to}}
    </li>
