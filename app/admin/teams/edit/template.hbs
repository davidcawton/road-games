{{outlet}}

<h2 class="edit-team-form__title">Edit The {{model.city}} {{model.name}}</h2>

{{#simple-form class="new-team-form" startingValues=model onsubmit="saveTeam" as |formValues|}}
  {{partial 'admin/teams/form'}}
<div class="edit-team-form__btns">
    {{mdl-button class='edit-form-btn' isFloating=true icon='check_circle'}}
    {{mdl-button class='edit-form-btn' action='removeTeam' isFloating=true icon='delete'}}
</div>
{{/simple-form}}
